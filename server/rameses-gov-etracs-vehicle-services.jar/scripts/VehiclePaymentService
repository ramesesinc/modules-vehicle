import com.rameses.annotations.*;

public class VehiclePaymentService {
	
	@Service("WorkflowTaskService")
	def taskSvc;

	@DataContext("vehicle_payment")
	def paymentEm;

	@ProxyMethod
	public void postPayment(def o ) {
		println ''
		println '** postPayment ';
		o.each{
			println '>> '+ it;
		}
	}

	@ProxyMethod
	public void voidPayment(def o ) {
		println ''
		println '** voidPayment ';
		o.each{
			println '>> '+ it;
		}
	}

}