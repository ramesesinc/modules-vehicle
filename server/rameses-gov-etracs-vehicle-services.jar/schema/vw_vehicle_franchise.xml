<schema adapter="vehicledb">
	<element tablename="vw_vehicle_franchise">
		<field name="objid" primary="true"/>
		<field name="state" />
		<field name="controlno" />
		<field name="vehicletypeid"	/>

		<complex name="vehicletype" ref="vehicletype" jointype="many-to-one" required="false">
			<key field="vehicletypeid" target="objid" />
		</complex> 
		
		<field name="currentyearid" />
		<field name="year" />
		<field name="geninfoid" />
		<field name="owner_objid" />
		<field name="owner_name" />
		<field name="owner_type" />
		<field name="owner_address_objid" />
		<field name="owner_address_text" />
		<field name="appid" />
		<field name="appstate" />
		<field name="startdate" />
		<field name="permit_permitno" />
		<field name="permit_permittype" />
		<field name="permit_dtissued" />
		<field name="permit_expirydate" />
		<field name="permit_issuedby_name" />
		<!--
		<complex name="units" ref="vw_vehicle_franchise_unit_active" jointype="one-to-many">
			<key field="objid" target="franchiseid" />
		</complex>
		-->
	</element>
</schema>

