<schema adapter="vehicledb">
	<element tablename="vw_vehicle_application">
		<field name="objid" primary="true" />

		<field name="franchiseid" />	
		<field name="vehicletypeid" />	

		<complex name="vehicletype" ref="vehicletype" jointype="many-to-one">
			<key field="vehicletypeid" target="objid" />
		</complex> 

		<field name="franchiseyearid" />	
		<field name="geninfoid" />	
		<field name="appno" searchable="true" caption="App No"/>	
		<field name="appdate" />	
		<field name="apptype" />	
		<field name="controlno" caption="Control No" searchable="true"/>	

		<field name="txnmode" />
		<field name="dtcreated" />	
		<field name="createdby_objid" />	
		<field name="createdby_name" />	
		<field name="particulars" />	
		<field name="taskid" />	
		<field name="dtapproved" />	
		<field name="permitid" />	
		<field name="year" />	
		<field name="owner_objid" />	
		<field name="owner_name" searchable="true" caption="Owner Name"/>	
		<field name="owner_address_objid" />	
		<field name="owner_address_text" />	
		<field name="owner_type" />

		<field name="phoneno" />	
		<field name="email" />	
		<field name="mobileno" />
		

		<field name="task_state" />	
		<field name="task_startdate" />	
		<field name="task_enddate" />	
		<field name="task_assignee_objid" />	
		<field name="task_assignee_name" />	
		<field name="task_actor_objid" />	
		<field name="task_actor_name" />	
		<field name="renewaldate" />

		<field name="permit_permitno" />
		<field name="permit_permittype" />
		<field name="permit_dtissued" />
		<field name="permit_expirydate" />
		<field name="permit_issuedby_name" />

		<field name="prev_year" />
		<field name="prev_franchiseyearid" />
		

		<complex name="units" ref="vehicle_application_unit" jointype="one-to-many">
			<key field="objid" target="appid" />
		</complex>
		<complex name="droppedunits" ref="vehicle_application_unit_dropped" jointype="one-to-many">
			<key field="objid" target="appid" />
		</complex>	

	</element>
</schema>