<schema adapter="vehicledb">
	<element tablename="vw_vehicle_franchise_unit_active">
		<field name="objid" primary="true" />

		<field name="franchiseid" />
		<field name="appid" />
		<field name="appno" />
		<field name="apptype" />		

		<complex name="unit" ref="vehicle_unit" jointype="many-to-one" required="false">
			<key field="unitid" target="objid" />
		</complex> 

	</element>
</schema>