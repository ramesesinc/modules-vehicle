<schema adapter="vehicledb">
	<element tablename="vehicle_application_unit">
		<field name="objid" primary="true" prefix="VAUNIT"/>
		<field name="appid" required="true"/>
		<complex name="unit" ref="vehicle_unit" jointype="many-to-one" required="true">
			<key field="unitid" target="objid" />
		</complex> 
	</element>
</schema>