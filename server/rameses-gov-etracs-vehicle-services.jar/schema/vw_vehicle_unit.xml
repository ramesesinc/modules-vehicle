<schema adapter="vehicledb">
	<element tablename="vw_vehicle_unit">
		<field name="objid" primary="true"/>
		<field name="year" caption="Year"/>
		<field name="controlno" caption="Control No" searchable="true"/>
		<field name="owner_objid" visible="false"/>		
		<field name="owner_name" caption="Owner Name" searchable="true"/>

		<field name="plateno" caption="Plate No" searchable="true"/>
		<field name="engineno" caption="Engine No"/>
		<field name="bodyno" caption="Body No" />
		<field name="sidecarno" caption="Seidecar No"/>
		<field name="make" caption="Make"/>
		<field name="model" caption="Model"/>
		<field name="color"/>
		<field name="chassisno"/>
		<field name="sidecarcolor"/>
		<field name="crname"/>
		<field name="remarks"/>

		<field name="vehicletype_objid" />
		<field name="vehicletype_title" />
		<field name="vehicletype_allowedfields"/>

	</element>
</schema>
			