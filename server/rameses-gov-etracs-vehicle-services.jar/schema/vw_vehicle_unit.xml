<schema adapter="vehicledb">
	<element tablename="vw_vehicle_unit">
		<field name="objid" primary="true"/>
		<field name="year" caption="Year"/>
		<field name="controlno" caption="Control No"/>
		<field name="owner_name" caption="Owner Name"/>
		<field name="plateno" caption="Plate No"/>
		<field name="engineno" caption="Engine No"/>
		<field name="bodyno" caption="Body No" visible="false"/>
		<field name="sidecarno" caption="Seidecar No" visible="false"/>
		<field name="make" caption="Make" visible="false"/>
		<field name="model" caption="Model" visible="false"/>
		<field name="color" visible="false"/>
		<field name="chassisno" visible="false"/>
		<field name="sidecarcolor" visible="false"/>
		<field name="crname" visible="false"/>
		<field name="driver_objid" visible="false"/>
		<field name="driver_name" caption="Driver Name"/>
		<field name="remarks" visible="false"/>
		
		<field name="owner_objid" visible="false"/>
			
		<field name="vehicletype_objid" visible="false"/>	
		<field name="vehicletype_title" visible="false"/>	
		<field name="vehicletype_allowedfields" visible="false"/>	
	</element>
</schema>

