<schema adapter="vehicledb">
	<element tablename="vehicle_permit">
		<field name="objid" primary="true" prefix="VPERM"/>
		<field name="state" required="true"/>
		<field name="permitno" required="true"/>
		<field name="permittype" required="true"/>
		<field name="dtissued" required="true"/>
		<field name="issuedby_objid" required="true"/>
		<field name="issuedby_name" required="true"/>
		<field name="expirydate" />
		<field name="appid" required="true"/>
		<field name="txnmode" required="true"/>

		<complex name="units" ref="vw_vehicle_unit_snapshot" jointype="one-to-many" required="false">
			<key field="objid" target="permitid" />
		</complex>
			
	</element>
</schema>