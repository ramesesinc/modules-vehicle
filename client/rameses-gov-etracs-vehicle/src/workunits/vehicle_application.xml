<workunit schemaName="vw_vehicle_application" processName="vehicle_application">
    <invokers>
        <invoker type="vw_vehicle_application:open" 
                 action="open" caption="Vehicle Application"/>
        
        <invoker type="formActions" action="assess" caption="Assess" 
                 visibleWhen="#{ showAssessAction == true }"/>

        <invoker type="formActions" 
                action="issuePermit" caption="Issue Permit" 
                visibleWhen="#{ showIssuePermitAction == true &amp;&amp; entity.permitid == null }"/>
        
        <!--
        <invoker type="formActions" 
                 action="printApplication" caption="Print" 
                 xvisibleWhen="#{entity.permitid == null &amp;&amp; entity.task.state.toString().matches('release|end') }"/>
        -->
        <invoker type="formActions" action="viewReport" tooltip="View Reports" caption="Reports"/>

    </invokers>
    <code class="com.rameses.gov.etracs.vehicle.models.ApplicationModel" />
    <pages>
        <page template="com.rameses.gov.etracs.vehicle.views.ApplicationFormPage" />
    </pages>
</workunit>