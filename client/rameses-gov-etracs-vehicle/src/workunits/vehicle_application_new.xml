<workunit schemaName="vehicle_application">
    <invokers>
        <invoker folderid="menucategory/vehicle/application"  apptype="NEW"
                 action="startOnline" caption="New Application"  index="10" role="INFO"/>
    </invokers>
    <code class="com.rameses.gov.etracs.vehicle.models.ApplicationEntryModel" />
    <pageflow>
        <start>
            <transition to="entry" />
        </start>
        <page name="entry" title="#{vehicletype.title} Application Initial (#{entity.apptype} #{entity.txnmode})">
            <transition to="end" caption="Submit" action="save"  
                        confirm="You are about to create this application. Proceed?"/>
        </page>
        <end/>
    </pageflow>
    <pages>
        <page name="entry" template="com.rameses.gov.etracs.vehicle.views.ApplicationEntryPage" />
    </pages>
</workunit>